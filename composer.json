{
    "name": "ikerib/zertegi",
    "description": "Pasaiako udal artxiboa kudeatzeko aplikazioa",
    "type": "project",
    "license": "proprietary",
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-ldap": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.13.3",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13.1",
        "friendsofsymfony/ckeditor-bundle": "^2.4.0",
        "friendsofsymfony/jsrouting-bundle": "3.0.0",
        "friendsofsymfony/rest-bundle": "^3.3.0",
        "jms/serializer-bundle": "^4.0.2",
        "knplabs/knp-paginator-bundle": "^v5.8.0",
        "knplabs/knp-snappy-bundle": "^v1.9.0",
        "laminas/laminas-zendframework-bridge": "^1.6",
        "nelmio/cors-bundle": "^2.2.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "sensio/framework-extra-bundle": "^6.2.6",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^v1.0.1",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.1",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/ldap": "5.4.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "5.4.*",
        "twig/string-extra": "^3.12"
    },
    "require-dev": {
        "deployer/deployer": "^v6.4.7",
        "phpunit/phpunit": "^9.5.21",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },"allow-plugins": {
            "symfony/flex": true,
            "ocramius/package-versions": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,

            "require": "5.4.*"
        }
    }
}
