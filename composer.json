{
    "name": "ikerib/zertegi",
    "description": "Pasaiako udal artxiboa kudeatzeko aplikazioa",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-ldap": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^2.5",
        "jms/serializer-bundle": "^3.4",
        "knplabs/knp-paginator-bundle": "^4.0",
        "knplabs/knp-snappy-bundle": "^1.6",
        "laminas/laminas-zendframework-bridge": "^1.6",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "sensio/framework-extra-bundle": "^5.4",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/ldap": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/process": "4.3.*",
        "symfony/property-access": "4.3.*",
        "symfony/property-info": "4.3.*",
        "symfony/proxy-manager-bridge": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/serializer": "4.3.*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.3.*",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/web-link": "4.3.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "4.3.*",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "codeception/codeception": "^3.0",
        "deployer/deployer": "^6.4",
        "phpunit/phpunit": "^8.5",
        "symfony/browser-kit": "4.3.*",
        "symfony/css-selector": "4.3.*",
        "symfony/debug-bundle": "4.3.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/panther": "^0.4.1",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "4.3.*",
        "symfony/web-profiler-bundle": "4.3.*",
        "symfony/web-server-bundle": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },"allow-plugins": {
            "symfony/flex": true,
            "ocramius/package-versions": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
