{% extends 'base.html.twig' %}

{% block title %}Obra txikiak{% endblock %}
{% block header %}Obra txikiak{% endblock %}
{% block header_desc %}{{ obratxikiak.id }} ikusten{% endblock %}
{% block breadcrumbs %}
    <li class="active">Obra txikiak</li>
    <li class="active">Ikusi</li>
{% endblock %}

{% block body %}
    <div class="row">

        <div class="col-xs-12">

            <div class="box">
                <div class="box-header">
                    <div class="box-tools box-tools-left" data-toggle="tooltip" title="" data-original-title="Status">
                        <a class="btn btn-sm btn-primary" href="{{ path('obratxikiak_edit', {'id': obratxikiak.id}) }}"><i class="fas fa-edit"></i> Aldatu</a>
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">&nbsp;</div>
                    <table class="table">
                        <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ obratxikiak.id }}</td>
                        </tr>
                        <tr>
                            <th>Espedientea</th>
                            <td>{{ obratxikiak.espedientea }}</td>
                        </tr>
                        <tr>
                            <th>Sailkapena</th>
                            <td>{{ obratxikiak.sailkapena }}</td>
                        </tr>
                        <tr>
                            <th>Signatura</th>
                            <td>{{ obratxikiak.signatura }}</td>
                        </tr>
                        <tr>
                            <th>Urtea</th>
                            <td>{{ obratxikiak.urtea }}</td>
                        </tr>
                        <tr>
                            <th>Numdoc</th>
                            <td>{{ obratxikiak.numdoc }}</td>
                        </tr>
                        <tr>
                            <th>Knosysid</th>
                            <td>{{ obratxikiak.knosysid }}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('jsDeleteForm') }}
{% endblock %}
