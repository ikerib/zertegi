<form method="post" action="{{ path('protokoloak_delete', {'id': protokoloak.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ protokoloak.id) }}">
    <button class="btn">Delete</button>
</form>
