{% extends 'base.html.twig' %}

{% block title %}Protokoloak{% endblock %}
{% block header %}Protokoloak{% endblock %}
{% block header_desc %}{{ protokoloak.id }} ikusten{% endblock %}
{% block breadcrumbs %}
    <li class="active">Protokoloak</li>
    <li class="active">Ikusi</li>
{% endblock %}

{% block body %}
    <div class="row">

        <div class="col-xs-12">

            <div class="box">
                <div class="box-header">
                    <div class="box-tools box-tools-left" data-toggle="tooltip" title="" data-original-title="Status">
                        <a class="btn btn-sm btn-primary" href="{{ path('protokoloak_edit', {'id': protokoloak.id}) }}"><i class="fas fa-edit"></i> Aldatu</a>
                        <a class="btn btn-sm btn-default" href="{{ path('protokoloak_index') }}"><i class="fa fa-fw fa-arrow-alt-circle-left" aria-hidden="true"></i> {{ 'Zerrendara itzuli' | trans }}</a>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-default" data-toggle="modal" data-target="#modal-default">
                                Etiketa inprimatu
                            </button>
                            <a id="btnPrint" class="btn btn-sm btn-default" href="{{ path('protokoloak_print', {'id': protokoloak.id}) }}"><i class="fa fa-fw fa-print"></i> {{ "Inprimitu" | trans }}</a>
                        </div>
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">&nbsp;</div>
                    <table class="table">
                        <tbody>
                        <tr>
                            <th>Artxiboa</th>
                            <td>{{ protokoloak.artxiboa  | raw }}</td>
                        </tr>
                        <tr>
                            <th>Saila</th>
                            <td>{{ protokoloak.saila | raw  }}</td>
                        </tr>
                        <tr>
                            <th>Signatura</th>
                            <td>{{ protokoloak.signatura  | raw }}</td>
                        </tr>
                        <tr>
                            <th>Eskribaua</th>
                            <td>{{ protokoloak.eskribaua | raw  }}</td>
                        </tr>
                        <tr>
                            <th>Data</th>
                            <td>{{ protokoloak.data | raw  }}</td>
                        </tr>
                        <tr>
                            <th>Laburpena</th>
                            <td>{{ protokoloak.laburpena  | raw }}</td>
                        </tr>
                        <tr>
                            <th>Datuak</th>
                            <td>{{ protokoloak.datuak  | raw }}</td>
                        </tr>
                        <tr>
                            <th>Oharrak</th>
                            <td>{{ protokoloak.oharrak | raw }}</td>
                        </tr>
                        <tr>
                            <th>Bilatzaileak</th>
                            <td>{{ protokoloak.bilatzaileak | raw  }}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

    {{ include('protokoloak/_apli_modal.html.twig') }}

{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('jsDeleteForm') }}
{% endblock %}
