{% extends 'base.html.twig' %}

{% block title %}Protokoloak{% endblock %}
{% block header %}Protokoloak{% endblock %}
{% block header_desc %}{{ protokoloak.id }} ikusten{% endblock %}
{% block breadcrumbs %}
    <li class="active">Protokoloak</li>
    <li class="active">Ikusi</li>
{% endblock %}

{% block body %}
    <div class="row">

        <div class="col-xs-12">

            <div class="box">
                <div class="box-header">
                    <div class="box-tools box-tools-left" data-toggle="tooltip" title="" data-original-title="Status">
                        <a class="btn btn-sm btn-primary" href="{{ path('protokoloak_edit', {'id': protokoloak.id}) }}"><i class="fas fa-edit"></i> Aldatu</a>
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">&nbsp;</div>
                    <table class="table">
                        <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ protokoloak.id }}</td>
                        </tr>
                        <tr>
                            <th>Artxiboa</th>
                            <td>{{ protokoloak.artxiboa }}</td>
                        </tr>
                        <tr>
                            <th>Saila</th>
                            <td>{{ protokoloak.saila }}</td>
                        </tr>
                        <tr>
                            <th>Signatura</th>
                            <td>{{ protokoloak.signatura }}</td>
                        </tr>
                        <tr>
                            <th>Eskribaua</th>
                            <td>{{ protokoloak.eskribaua }}</td>
                        </tr>
                        <tr>
                            <th>Data</th>
                            <td>{{ protokoloak.data }}</td>
                        </tr>
                        <tr>
                            <th>Laburpena</th>
                            <td>{{ protokoloak.laburpena }}</td>
                        </tr>
                        <tr>
                            <th>Datuak</th>
                            <td>{{ protokoloak.datuak }}</td>
                        </tr>
                        <tr>
                            <th>Oharrak</th>
                            <td>{{ protokoloak.oharrak }}</td>
                        </tr>
                        <tr>
                            <th>Bilatzaileak</th>
                            <td>{{ protokoloak.bilatzaileak }}</td>
                        </tr>
                        <tr>
                            <th>Numdoc</th>
                            <td>{{ protokoloak.numdoc }}</td>
                        </tr>
                        <tr>
                            <th>Knosysid</th>
                            <td>{{ protokoloak.knosysid }}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('jsDeleteForm') }}
{% endblock %}
